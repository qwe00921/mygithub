    <style name="global_tab_item">
        <item name="android:background">@drawable/global_tab_item</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/font_size_30xp</item>
        <item name="android:textColor">@color/global_tab_item</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1</item>
    </style>
    
      <style name="orderlist_tab_left">
        <item name="android:background">@drawable/orderlist_tab_left</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">@dimen/font_size_30xp</item>
